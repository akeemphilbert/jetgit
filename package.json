{
  "name": "jetgit",
  "displayName": "JetGit",
  "description": "JetBrains IDE-style Git functionality for VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "SCM Providers"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jetgit.showGitMenu",
        "title": "JetGit: Show Git Menu",
        "category": "JetGit",
        "icon": "$(source-control)"
      },
      {
        "command": "jetgit.updateProject",
        "title": "JetGit: Update Project",
        "category": "JetGit"
      },
      {
        "command": "jetgit.commitChanges",
        "title": "JetGit: Commit Changes",
        "category": "JetGit"
      },
      {
        "command": "jetgit.push",
        "title": "JetGit: Push",
        "category": "JetGit"
      },
      {
        "command": "jetgit.fetch",
        "title": "JetGit: Fetch",
        "category": "JetGit"
      },
      {
        "command": "jetgit.newBranch",
        "title": "JetGit: New Branch",
        "category": "JetGit"
      },
      {
        "command": "jetgit.checkoutRevision",
        "title": "JetGit: Checkout Revision",
        "category": "JetGit"
      },
      {
        "command": "jetgit.newBranchFrom",
        "title": "JetGit: New Branch From",
        "category": "JetGit"
      },
      {
        "command": "jetgit.showDiffWithWorkingTree",
        "title": "JetGit: Show Diff with Working Tree",
        "category": "JetGit"
      },
      {
        "command": "jetgit.updateBranch",
        "title": "JetGit: Update Branch",
        "category": "JetGit"
      },
      {
        "command": "jetgit.pushBranch",
        "title": "JetGit: Push Branch",
        "category": "JetGit"
      },
      {
        "command": "jetgit.renameBranch",
        "title": "JetGit: Rename Branch",
        "category": "JetGit"
      },
      {
        "command": "jetgit.showFileDiff",
        "title": "JetGit: Show File Diff",
        "category": "JetGit"
      },
      {
        "command": "jetgit.compareWithBranch",
        "title": "JetGit: Compare with Branch",
        "category": "JetGit"
      },
      {
        "command": "jetgit.context.pull",
        "title": "Pull"
      },
      {
        "command": "jetgit.context.push",
        "title": "Push"
      },
      {
        "command": "jetgit.context.fetch",
        "title": "Fetch"
      },
      {
        "command": "jetgit.context.merge",
        "title": "Merge"
      },
      {
        "command": "jetgit.context.rebase",
        "title": "Rebase"
      },
      {
        "command": "jetgit.context.branches",
        "title": "Branches"
      },
      {
        "command": "jetgit.context.newBranch",
        "title": "New Branch"
      },
      {
        "command": "jetgit.context.newTag",
        "title": "New Tag"
      },
      {
        "command": "jetgit.context.showHistory",
        "title": "Show History"
      },
      {
        "command": "jetgit.context.showCurrentVersion",
        "title": "Show Current Version"
      },
      {
        "command": "jetgit.context.compareWithBranch",
        "title": "Compare with Branch"
      },
      {
        "command": "jetgit.context.compareWithRevision",
        "title": "Compare with Revision"
      },
      {
        "command": "jetgit.context.annotate",
        "title": "Annotate"
      },
      {
        "command": "jetgit.context.revert",
        "title": "Revert"
      },
      {
        "command": "jetgit.context.resetHead",
        "title": "Reset HEAD"
      },
      {
        "command": "jetgit.context.stashChanges",
        "title": "Stash Changes"
      },
      {
        "command": "jetgit.context.unstashChanges",
        "title": "Unstash Changes"
      },
      {
        "command": "jetgit.context.manageRemotes",
        "title": "Manage Remotes"
      },
      {
        "command": "jetgit.refreshStatus",
        "title": "JetGit: Refresh Git Status",
        "category": "JetGit"
      },
      {
        "command": "jetgit.showExtensionInfo",
        "title": "JetGit: Show Extension Information",
        "category": "JetGit"
      }
    ],
    "submenus": [
      {
        "id": "jetgit.context.submenu",
        "label": "Git",
        "icon": "$(source-control)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "jetgit.showGitMenu",
          "when": "gitOpenRepositoryCount != 0"
        },
        {
          "command": "jetgit.updateProject",
          "when": "gitOpenRepositoryCount != 0"
        },
        {
          "command": "jetgit.commitChanges",
          "when": "gitOpenRepositoryCount != 0"
        },
        {
          "command": "jetgit.push",
          "when": "gitOpenRepositoryCount != 0"
        },
        {
          "command": "jetgit.fetch",
          "when": "gitOpenRepositoryCount != 0"
        },
        {
          "command": "jetgit.newBranch",
          "when": "gitOpenRepositoryCount != 0"
        },
        {
          "command": "jetgit.checkoutRevision",
          "when": "gitOpenRepositoryCount != 0"
        },
        {
          "command": "jetgit.refreshStatus",
          "when": "gitOpenRepositoryCount != 0"
        },
        {
          "command": "jetgit.showExtensionInfo"
        }
      ],
      "editor/title": [
        {
          "command": "jetgit.showGitMenu",
          "when": "gitOpenRepositoryCount != 0",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "submenu": "jetgit.context.submenu",
          "when": "gitOpenRepositoryCount != 0",
          "group": "2_workspace"
        }
      ],
      "editor/context": [
        {
          "submenu": "jetgit.context.submenu",
          "when": "gitOpenRepositoryCount != 0",
          "group": "1_modification"
        }
      ],
      "jetgit.context.submenu": [
        {
          "command": "jetgit.context.pull",
          "group": "1_repository@1"
        },
        {
          "command": "jetgit.context.push",
          "group": "1_repository@2"
        },
        {
          "command": "jetgit.context.fetch",
          "group": "1_repository@3"
        },
        {
          "command": "jetgit.context.merge",
          "group": "1_repository@4"
        },
        {
          "command": "jetgit.context.rebase",
          "group": "1_repository@5"
        },
        {
          "command": "jetgit.context.branches",
          "group": "2_branch@1"
        },
        {
          "command": "jetgit.context.newBranch",
          "group": "2_branch@2"
        },
        {
          "command": "jetgit.context.newTag",
          "group": "2_branch@3"
        },
        {
          "command": "jetgit.context.showHistory",
          "group": "3_file@1"
        },
        {
          "command": "jetgit.context.showCurrentVersion",
          "group": "3_file@2"
        },
        {
          "command": "jetgit.context.compareWithBranch",
          "group": "3_file@3"
        },
        {
          "command": "jetgit.context.compareWithRevision",
          "group": "3_file@4"
        },
        {
          "command": "jetgit.context.annotate",
          "group": "3_file@5"
        },
        {
          "command": "jetgit.context.revert",
          "group": "3_file@6"
        },
        {
          "command": "jetgit.context.resetHead",
          "group": "4_advanced@1"
        },
        {
          "command": "jetgit.context.stashChanges",
          "group": "4_advanced@2"
        },
        {
          "command": "jetgit.context.unstashChanges",
          "group": "4_advanced@3"
        },
        {
          "command": "jetgit.context.manageRemotes",
          "group": "4_advanced@4"
        }
      ]
    },
    "keybindings": [
      {
        "command": "jetgit.showGitMenu",
        "key": "ctrl+shift+g ctrl+shift+m",
        "mac": "cmd+shift+g cmd+shift+m",
        "when": "gitOpenRepositoryCount != 0"
      },
      {
        "command": "jetgit.updateProject",
        "key": "ctrl+shift+g u",
        "mac": "cmd+shift+g u",
        "when": "gitOpenRepositoryCount != 0"
      },
      {
        "command": "jetgit.commitChanges",
        "key": "ctrl+shift+g c",
        "mac": "cmd+shift+g c",
        "when": "gitOpenRepositoryCount != 0"
      },
      {
        "command": "jetgit.push",
        "key": "ctrl+shift+g p",
        "mac": "cmd+shift+g p",
        "when": "gitOpenRepositoryCount != 0"
      },
      {
        "command": "jetgit.fetch",
        "key": "ctrl+shift+g f",
        "mac": "cmd+shift+g f",
        "when": "gitOpenRepositoryCount != 0"
      },
      {
        "command": "jetgit.newBranch",
        "key": "ctrl+shift+g b",
        "mac": "cmd+shift+g b",
        "when": "gitOpenRepositoryCount != 0"
      },
      {
        "command": "jetgit.compareWithBranch",
        "key": "ctrl+shift+g d",
        "mac": "cmd+shift+g d",
        "when": "editorTextFocus && gitOpenRepositoryCount != 0"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "esbuild src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "watch": "npm run compile -- --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e --timeout=300000",
    "test:performance": "jest --testPathPattern=performance --timeout=300000",
    "test:all": "ts-node tests/test-runner.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/jest": "^29.5.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "babel-jest": "^30.0.5",
    "esbuild": "^0.17.0",
    "eslint": "^8.28.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^4.9.4"
  }
}
